<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="com.keyware.unit.core.ICoverageSession"
            class="com.keyware.unit.internal.ui.WorkbenchAdapterFactory">
         <adapter type="org.eclipse.ui.model.IWorkbenchAdapter"/>
      </factory>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchGroups">
      <launchGroup
            bannerImage="$nl$/icons/full/wizban/run_coverage.png"
            id="com.keyware.unit.ui.launchGroup.coverage"
            image="$nl$/icons/full/elcl16/runcoverage.png"
            label="%CoverageLaunchGroup.label"
            mode="coverage"
            title="%CoverageLaunchGroup.title"/>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.keyware.unit.internal.ui.actions.ContextualLaunchableTester"
            id="com.keyware.unit.ui.propertyTester.contextualLaunchable"
            namespace="com.keyware.unit.ui"
            properties="launchable"
            type="java.lang.Object"/>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            label="%JavaApplicationShortcut.label"
            icon="$nl$/icons/full/etool16/java_launch.png"
            modes="coverage"
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.eclipse.jdt.debug.ui.localJavaShortcut"
            id="com.keyware.unit.ui.localJavaShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.keyware.unit.ui.launchable" args="org.eclipse.jdt.debug.ui.localJavaShortcut"/>
             </with>
           </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            label="%JUnitTestShortcut.label"
            icon="$nl$/icons/full/etool16/junit_launch.png"
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.eclipse.jdt.junit.junitShortcut"
            modes="coverage"
            id="com.keyware.unit.ui.junitShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.keyware.unit.ui.launchable" args="org.eclipse.jdt.junit.junitShortcut"/>
             </with>
           </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.testng.eclipse.shortcut"
            icon="$nl$/icons/full/etool16/testng_launch.gif"
            id="com.keyware.unit.ui.testNgShortcut"
            label="%TestNgShortcut.label"
            modes="coverage">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <test
                        args="org.testng.eclipse.shortcut"
                        property="com.keyware.unit.ui.launchable">
                  </test>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.testng.eclipse.shortcut.testngSuite"
            icon="$nl$/icons/full/etool16/testng_launch.gif"
            id="com.keyware.unit.ui.testNgSuiteShortcut"
            label="%TestNgSuiteShortcut.label"
            modes="coverage">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <test
                        args="org.testng.eclipse.shortcut.testngSuite"
                        property="com.keyware.unit.ui.launchable">
                  </test>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            label="%EclipseApplicationShortcut.label"
            icon="$nl$/icons/full/etool16/eclipse_launch.png"
            modes="coverage"
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.eclipse.pde.ui.runtimeWorkbenchShortcut"
            id="com.keyware.unit.ui.workbenchShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.keyware.unit.ui.launchable" args="org.eclipse.pde.ui.runtimeWorkbenchShortcut"/>
             </with>
           </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            label="%JUnitPluginTestShortcut.label"
            icon="$nl$/icons/full/etool16/junitplugin_launch.png"
            modes="coverage"
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.eclipse.pde.ui.junitWorkbenchShortcut"
            id="com.keyware.unit.ui.junitPluginShortcut">
         <contextualLaunch>
           <enablement>
             <with variable="selection">
               <test property="com.keyware.unit.ui.launchable" args="org.eclipse.pde.ui.junitWorkbenchShortcut"/>
             </with>
           </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            label="%JUnitRAPTestShortcut.label "
            icon="$nl$/icons/full/etool16/junitplugin_launch.png"
            modes="coverage"
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.eclipse.rap.ui.launch.RAPJUnitLaunchShortcut"
            id="com.keyware.unit.ui.RAPJUnitLaunchShortcut">
         <contextualLaunch>
            <enablement>
               <with variable="selection">
                  <test property="com.keyware.unit.ui.launchable" args="org.eclipse.rap.ui.launch.RAPJUnitLaunchShortcut"/>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.eclipse.swtbot.eclipse.ui.junitShortcut"
            icon="$nl$/icons/full/etool16/swtbot_launch.png"
            id="com.keyware.unit.ui.SWTBotJUnitShortcut"
            label="%SWTBotJUnitShortcut.label"
            modes="coverage">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <test
                        args="org.eclipse.swtbot.eclipse.ui.junitShortcut"
                        property="com.keyware.unit.ui.launchable">
                  </test>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="com.keyware.unit.ui.launching.CoverageLaunchShortcut:org.eclipse.shortcut.scala"
            icon="$nl$/icons/full/etool16/scala_launch.png"
            id="com.keyware.unit.ui.scalaShortcut"
            label="%ScalaApplicationShortcut.label"
            modes="coverage">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <test
                        args="org.eclipse.shortcut.scala"
                        property="com.keyware.unit.ui.launchable">
                  </test>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="com.keyware.unit.ui.CoverageActionSet"
            label="%CoverageActionSet.label"
            visible="false">
         <menu
               id="org.eclipse.ui.run"
               label="%RunMenu.label"
               path="additions">
            <separator name="relaunchGroup"/>
            <separator name="emptyLaunchGroup"/>
         </menu>
         <action
               class="com.keyware.unit.internal.ui.actions.CoverageToolbarAction"
               disabledIcon="$nl$/icons/full/dlcl16/runcoverage.png"
               hoverIcon="$nl$/icons/full/elcl16/runcoverage.png"
               icon="$nl$/icons/full/elcl16/runcoverage.png"
               id="com.keyware.unit.ui.actions.CoverageDropDownAction"
               label="%CoverageDropDownAction.label"
               style="pulldown"
               toolbarPath="org.eclipse.debug.ui.launchActionSet/debug"/>
         <action
               class="com.keyware.unit.internal.ui.actions.OpenCoverageConfigurations"
               definitionId="com.keyware.unit.ui.commands.OpenCoverageConfiguration"
               id="com.keyware.unit.ui.actions.OpenCoverageConfiguration"
               label="%OpenCoverageConfigurationsAction.label"
               menubarPath="org.eclipse.ui.run/emptyLaunchGroup"/>
         <action
               class="com.keyware.unit.internal.ui.actions.CoverageAsAction"
               id="com.keyware.unit.ui.actions.CoverageAsAction"
               label="%CoverageAsAction.label"
               menubarPath="org.eclipse.ui.run/emptyLaunchGroup"
               style="pulldown"/>
         <action
               class="com.keyware.unit.internal.ui.actions.CoverageHistoryAction"
               id="com.keyware.unit.ui.actions.CoverageHistoryAction"
               label="%CoverageHistoryAction.label"
               menubarPath="org.eclipse.ui.run/emptyLaunchGroup"
               style="pulldown"/>
         <action
               class="com.keyware.unit.internal.ui.actions.CoverageLastAction"
               definitionId="com.keyware.unit.ui.commands.CoverageLast"
               disabledIcon="$nl$/icons/full/dlcl16/relaunch.png"
               hoverIcon="$nl$/icons/full/elcl16/relaunch.png"
               icon="$nl$/icons/full/elcl16/relaunch.png"
               id="com.keyware.unit.ui.actions.CoverageLast"
               label="%CoverageLastAction.label"
               menubarPath="org.eclipse.ui.run/relaunchGroup"/>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.keyware.unit.ui"
            name="%EclEmmaCommandCategory.name">
      </category>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageLastCommand.description"
            id="com.keyware.unit.ui.commands.CoverageLast"
            name="%CoverageLastCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%OpenCoverageConfigurationsCommand.description"
            id="com.keyware.unit.ui.commands.OpenCoverageConfiguration"
            name="%OpenCoverageConfigurationsCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJavaApplicationCommand.description"
            id="com.keyware.unit.ui.localJavaShortcut.coverage"
            name="%CoverageJavaApplicationCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJUnitTestCommand.description"
            id="com.keyware.unit.ui.junitShortcut.coverage"
            name="%CoverageJUnitTestCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageTestNGCommand.description"
            id="com.keyware.unit.ui.testNgShortcut.coverage"
            name="%CoverageTestNGCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageTestNGSuiteCommand.description"
            id="com.keyware.unit.ui.testNgSuiteShortcut.coverage"
            name="%CoverageTestNGSuiteCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageEclipseApplicationCommand.description"
            id="com.keyware.unit.ui.workbenchShortcut.coverage"
            name="%CoverageEclipseApplicationCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJUnitPluginTestCommand.description"
            id="com.keyware.unit.ui.junitPluginShortcut.coverage"
            name="%CoverageJUnitPluginTestCommand.name"/>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageJUnitRAPTestCommand.description"
            id="com.keyware.unit.ui.junitRAPShortcut.coverage"
            name="%CoverageJUnitRAPTestCommand.name">
      </command>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageSWTBotJUnitCommand.description"
            id="com.keyware.unit.ui.swtBotJunitShortcut.coverage"
            name="%CoverageSWTBotJUnitCommand.name">
      </command>
      <command
            categoryId="org.eclipse.debug.ui.category.run"
            description="%CoverageScalaApplicationCommand.description"
            id="com.keyware.unit.ui.scalaShortcut.coverage"
            name="%CoverageScalaApplicationCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            id="com.keyware.unit.ui.selectRootElements"
            name="%SelectRootElementsCommand.name">
         <commandParameter
               id="type"
               name="type"
               optional="false">
         </commandParameter>
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            id="com.keyware.unit.ui.selectCounters"
            name="%SelectCountersCommand.name">
         <commandParameter
               id="type"
               name="type"
               optional="false">
         </commandParameter>
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            id="com.keyware.unit.ui.hideUnusedElements"
            name="%HideUnusedElementsCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.ImportSessionHandler"
            id="com.keyware.unit.ui.importSession"
            name="%ImportSessionCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.ExportSessionHandler"
            id="com.keyware.unit.ui.exportSession"
            name="%ExportSessionCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.RelaunchSessionHandler"
            id="com.keyware.unit.ui.relaunchSession"
            name="%RelaunchSessionCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.RemoveActiveSessionHandler"
            id="com.keyware.unit.ui.removeActiveSession"
            name="%RemoveActiveSessionCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.RemoveAllSessionsHandler"
            id="com.keyware.unit.ui.removeAllSessions"
            name="%RemoveAllSessionsCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.MergeSessionsHandler"
            id="com.keyware.unit.ui.mergeSessions"
            name="%MergeSessionsCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            id="com.keyware.unit.ui.linkWithSelection"
            name="%LinkWithSelectionCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.SelectActiveSessionHandler"
            id="com.keyware.unit.ui.selectActiveSession"
            name="%SelectActiveSessionCommand.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.OpenSessionExecutionDataHandler"
            id="com.keyware.unit.ui.openSessionExecutionData"
            name="%OpenSessionExecutionData.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.DumpExecutionDataHandler"
            id="com.keyware.unit.ui.dumpExecutionData"
            name="%DumpExecutionData.name">
      </command>
      <command
            categoryId="com.keyware.unit.ui"
            defaultHandler="com.keyware.unit.internal.ui.handlers.ResetOnDumpHandler"
            id="com.keyware.unit.ui.resetOnDump"
            name="%ResetOnDump.name">
      </command>
   </extension>

   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.keyware.unit.ui.localJavaShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E J"/>
      <key
            commandId="com.keyware.unit.ui.scalaShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E L">
      </key>
      <key
            commandId="com.keyware.unit.ui.junitShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E T"/>
      <key
            commandId="com.keyware.unit.ui.testNgShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E N">
      </key>
      <key
            commandId="com.keyware.unit.ui.testNgSuiteShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E G">
      </key>
      <key
            commandId="com.keyware.unit.ui.workbenchShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E E"/>
      <key
            commandId="com.keyware.unit.ui.junitPluginShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E P"/>
      <key
            commandId="com.keyware.unit.ui.junitRAPShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E R"/>
      <key
            commandId="com.keyware.unit.ui.swtBotJunitShortcut.coverage"
            contextId="org.eclipse.ui.globalScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M2+E S">
      </key>
      <key
            commandId="com.keyware.unit.ui.commands.CoverageLast"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+F11"/>
      <!-- override key binding on Mac OS X -->
      <key
            contextId="org.eclipse.ui.contexts.window"
            platform="carbon"
            sequence="M1+M2+F11"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            contextId="org.eclipse.ui.contexts.window"
            platform="cocoa"
            sequence="M1+M2+F11"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
      <key
            commandId="com.keyware.unit.ui.commands.CoverageLast"
            platform="carbon"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M4+F11"/>
      <key
            commandId="com.keyware.unit.ui.commands.CoverageLast"
            platform="cocoa"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M4+F11"/>
   </extension>

   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="org.eclipse.jdt.ui.JavaPerspective">
         <actionSet id="com.keyware.unit.ui.CoverageActionSet"/>
         <showInPart id="com.keyware.unit.ui.CoverageView"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.debug.ui.DebugPerspective">
         <actionSet id="com.keyware.unit.ui.CoverageActionSet"/>
         <showInPart id="com.keyware.unit.ui.CoverageView"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.pde.ui.PDEPerspective">
         <actionSet id="com.keyware.unit.ui.CoverageActionSet"/>
         <showInPart id="com.keyware.unit.ui.CoverageView"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.jst.j2ee.J2EEPerspective">
         <actionSet id="com.keyware.unit.ui.CoverageActionSet"/>
         <showInPart id="com.keyware.unit.ui.CoverageView"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.scala-ide.sdt.core.perspective">
         <actionSet id="com.keyware.unit.ui.CoverageActionSet"/>
         <showInPart id="com.keyware.unit.ui.CoverageView"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="org.eclipse.jdt.ui.java"
            class="com.keyware.unit.internal.ui.coverageview.CoverageView"
            icon="$nl$/icons/full/eview16/coverage.png"
            id="com.keyware.unit.ui.CoverageView"
            name="%coverageView.title"/>
   </extension>
