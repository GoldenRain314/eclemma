   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="com.keyware.unit.ui.launching.CoverageTabGroup"
            helpContextId="com.keyware.unit.ui.coverage_launch_context"
            id="com.keyware.unit.ui.JavaApplicationTabGroup"
            type="org.eclipse.jdt.launching.localJavaApplication">
         <launchMode
               description="%localJavaApplicationTabGroupDescription.coverage"
               mode="coverage"/>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="com.keyware.unit.ui.launching.CoverageTabGroup"
            helpContextId="com.keyware.unit.ui.coverage_launch_context"
            id="com.keyware.unit.ui.JUnitTabGroup"
            type="org.eclipse.jdt.junit.launchconfig">
         <launchMode
               description="%junitTabGroupDescription.coverage"
               mode="coverage"/>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="com.keyware.unit.ui.launching.CoverageTabGroup"
            helpContextId="com.keyware.unit.ui.coverage_launch_context"
            id="com.keyware.unit.ui.WorkbenchTabGroup"
            type="org.eclipse.pde.ui.RuntimeWorkbench">
